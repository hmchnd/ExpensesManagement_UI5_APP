{  
   "_version":"1.0.0",
   "sap.app":{  
      "_version":"1.0.0",
      "id":"com.bnsf.eam.tne",
      "type":"application",
      "i18n":"i18n/i18n.properties",
      "title":"{{tne.app.lbl.appTitle}}",
      "description":"{{tne.app.lbl.appDescription}}",
      "applicationVersion":{  
         "version":"17.1.0"
      },
      "dataSources":{  
         "userModel":{  
            "uri":"https://uid3.bnsf.com/sap/opu/odata/sap/ZPM_GW_TNA_USER_SRV",
            "type":"OData",
            "settings":{  
               "odataVersion":"2.0",
               "localUri":"localService/metadata.xml"
            }
         },
         "gangModel":{  
            "uri":"https://uid3.bnsf.com/sap/opu/odata/sap/ZPM_GW_TNA_GANGS_SRV",
            "type":"OData",
            "settings":{  
               "odataVersion":"2.0",
               "localUri":"localService/metadata.xml"
            }
         }
      }
   },
   "sap.ui":{  
      "_version":"1.0.0",
      "technology":"UI5",
      "icons":{  
         "icon":"resources/ETER16_16.png",
         "favIcon":"resources/ETER64_64.png",
         "phone":"resources/ETER16_16.png",
         "phone@2":"resources/ETER16_16.png",
         "tablet":"resources/ETER32_32.png",
         "tablet@2":"resources/ETER32_32.png"
      },
      "deviceTypes":{  
         "desktop":true,
         "tablet":true,
         "phone":true
      },
      "supportedThemes":[  
         "sap_bluecrystal"
      ]
   },
   "sap.ui5":{  
      "_version":"1.1.0",
      "rootView":{  
         "viewName":"com.bnsf.eam.tne.view.App",
         "type":"XML",
         "id":"tne.app"
      },
      "dependencies":{  
         "minUI5Version":"1.28.40",
         "libs":{  
            "sap.ui.core":{  
               "minVersion":"1.28.0"
            },
            "sap.m":{  
               "minVersion":"1.28.0"
            },
            "sap.ui.layout":{  
               "minVersion":"1.28.0"
            }
         }
      },
      "contentDensities":{  
         "compact":true,
         "cozy":true
      },
      "models":{  
         "i18nModel":{  
            "type":"sap.ui.model.resource.ResourceModel",
            "settings":{  
               "bundleUrl":"com.bnsf.eam.tne.i18n.messageBundle",
               "bundleName":"com.bnsf.eam.tne.i18n.messageBundle"
            }
         },
         "userModel":{  
            "type":"sap.ui.model.odata.ODataModel",
            "dataSource":"userModel",
            "preload":true,
            "settings":{  
               "json":true,
               "metadataUrlParams":{  
                  "sap-documentation":"heading"
               }
            }
         }
      },
      "routing":{  
         "config":{  
            "routerClass":"sap.m.routing.Router",
            "viewType":"XML",
            "viewPath":"com.bnsf.eam.tne.view",
            "transition":"slide",
            "controlId":"tne.app.home",
            "controlAggregation":"pages",
            "transition":"slide"
         },
         "routes":[  
            {  
               "pattern":"",
               "name":"Dashboard",
               "target":[  
                  "dashCal",
                  "dashDetails",
                  "dashMaster"
               ]
            },
            {  
               "pattern":"gang/{GangId}",
               "name":"DashDetails",
               "target":[  
                  "dashMaster",
                  "dashDetails"
               ]
            },
            {  
               "pattern":"gang/{GangId}/calendar",
               "name":"DashDetailsCal",
               "target":[  
                  "dashMaster",
                  "dashDetails",
                  "dashCal"
               ]
            },
            {  
               "pattern":"gang/{GangId}/messages",
               "name":"DashDetailsMsg",
               "target":[  
                  "dashMaster",
                  "dashDetails",
                  "dashMsg"
               ]
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/lineup",
               "name":"Lineup",
               "target":"lineup"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/emp/{EmpID}",
               "name":"LUEmployee",
               "target":"luEmployee"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/pos/{PosNo}",
               "name":"LUPosition",
               "target":"luposition"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/MultiEmp",
               "name":"LUMultiEmployee",
               "target":"lumultiemployee"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/activities",
               "name":"Activities",
               "target":"activities"
            },
            {  
               "pattern":"gangs/{GangId}/date/{WorkDate}/activities/details/{ActSeq}",
               "name":"ActivityDetails",
               "target":"actDetails"
            },
            {  
               "pattern":"gangs/{GangId}/date/{WorkDate}/activities/employee/{ActSeq}/{EmpID}",
               "name":"ActivityPeople",
               "target":"actPeople"
            },
             {  
               "pattern":"gangs/{GangId}/date/{WorkDate}/activities/workDay/{ActSeq}",
               "name":"WorkDayDetails",
               "target":"wrkDayDetail"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/paycodes",
               "name":"Paycode",
               "target":"paycode"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/paycodes/emp/{EmpID}",
               "name":"PCEmployee",
               "target":"pcEmp"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/paycode/emp/{EmpID}/expense/{ExpPC}",
               "name":"PCViewExp",
               "target":"pcViewExp"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/paycodes/addExpense",
               "name":"PCAddExp",
               "target":"pcAddExp"
            },
            {  
               "pattern":"gang/{GangId}/date/{WorkDate}/results",
               "name":"TimeResults",
               "target":"timeResults"
            }
         ],
         "targets":{  
            "dashboard":{  
               "viewPath":"com.bnsf.eam.tne.view",
               "viewName":"Dashboard"
            },
            "dashMaster":{  
               "parent":"dashboard",
               "viewPath":"com.bnsf.eam.tne.view",
               "viewName":"DashMaster",
               "controlId":"tne.dash.splitApp",
               "controlAggregation":"masterPages",
               "viewLevel":1
            },
            "dashDetails":{  
               "parent":"dashboard",
               "viewPath":"com.bnsf.eam.tne.view",
               "viewName":"DashDetail",
               "controlId":"tne.dash.splitApp",
               "controlAggregation":"detailPages",
               "viewLevel":2
            },
            "dashCal":{  
               "parent":"dashDetails",
               "viewPath":"com.bnsf.eam.tne.view",
               "viewName":"DashCalendar",
               "controlId":"tne.dashDet.page",
               "controlAggregation":"content",
               "viewLevel":3
            },
            "dashMsg":{  
               "parent":"dashDetails",
               "viewPath":"com.bnsf.eam.tne.view",
               "viewName":"DashMessages",
               "viewId":"DashMessagesId",
               "controlId":"tne.dashDet.page",
               "controlAggregation":"content",
               "viewLevel":3
            },
            "lineup":{  
               "viewName":"Lineup",
               "viewLevel":1
            },
            "luEmployee":{  
               "viewName":"LUEmployee",
               "viewLevel":2
            },
            "luposition":{  
               "viewName":"LUPosition",
               "viewLevel":2
            },
            "lumultiemployee":{  
               "viewName":"LUMultiEmployee",
               "viewLevel":2
            },
            "activities":{  
               "viewName":"Activities",
               "viewLevel":1
            },
            "actDetails":{  
               "viewName":"ActivityDetails",
               "viewLevel":2
            },
             "actPeople":{  
               "viewName":"ActivityPeople",
               "viewLevel":2
            },
            
            "wrkDayDetail":{  
               "viewName":"WorkDayDetails",
               "viewLevel":2
            },
            
            "paycode":{  
               "viewName":"Paycode",
               "viewLevel":1
            },
            "pcEmp":{  
               "viewName":"PCEmployee",
               "viewLevel":2
            },
            "pcViewExp":{  
               "viewName":"PCViewExp",
               "viewLevel":3
            },
            "pcAddExp":{  
               "viewName":"PCAddExp",
               "viewLevel":2
            },
            "timeResults":{  
               "viewName":"TimeResults",
               "viewLevel":4
            }
         }
      }
   }
}